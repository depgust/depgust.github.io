<script>
var playerInstance= jwplayer("player");
var ghttp = new XMLHttpRequest();
ghttp.onreadystatechange = function() {
if (this.readyState == 4 && this.status == 200) {
	var jsonResponse = JSON.parse(ghttp.responseText);
		var m3u8 = jsonResponse['url'];
		playerInstance.setup({
			file: m3u8,
			title : "F&uacute;tbol Libre TV - TNT Sports",
			description: "Mirar el canal TNT Sports de manera gratuita",
            image: "//librefutbol.com/img/tnt_back-footer.png",
			type: "hls",
			withCredentials: true,
			width: "100%",
			height: "100%",
			aspectratio: "16:9",
			autostart: false,
			cast: {},
			sharing: {}
		});
	}
}
ghttp.open("GET", "/tnt.json?" + Math.random().toString(36).substring(7), true);
ghttp.send();
</script>
</body>
